<?xml version="1.0" encoding="utf-8" ?>
<configuration>
  <configSections>
      <section name="PureConfig" type="PureSeeder.Core.Configuration.PureConfigSection, PureSeeder.Core, Version=1.0.0.0, Culture=neutral, PublicKeyToken=null"/>
  </configSections>
  <startup> 
      <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.5" />
  </startup>
  <PureConfig xmlns="urn:PureSeeder.Core.Configuration">
    <servers>
      <server name="Pure Server 1 - 64 Player Conquest" address="http://bf4-server1.purebattlefield.org/" minPlayers="32" maxPlayers="64"/>
      <server name="Pure Server 2 - 32 Player Mixed Mode" address="http://bf4-server2.purebattlefield.org/" minPlayers="18" maxPlayers="32"/>
    </servers>
    <settings>
      <add name="SleepWhenNotSeeding" value="1"/>
      <add name="SleepWhenSeeding" value="1"/>
      <add name="DisplayPlayerCount" value="true"/>
      <add name="EnableLogging" value="false"/>
      <add name="EnableGameHangProtection" value="true"/>
    </settings>
  </PureConfig>
</configuration>